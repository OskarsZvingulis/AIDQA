# Supabase Edge Function Secrets
# These will be set using: npx supabase secrets set --env-file supabase/.env

# Core Supabase Configuration (automatically injected by Supabase)
SUPABASE_URL=https://eboaqtbktyaxzrbcntzy.supabase.co
SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY_HERE

# Storage
STORAGE_BUCKET=visual

# Browserless Screenshot Service (lightweight REST API)
# Get free tier at: https://www.browserless.io/ (6hrs/month free)
BROWSERLESS_REST_ENDPOINT=https://chrome.browserless.io
BROWSERLESS_API_KEY=YOUR_BROWSERLESS_TOKEN_HERE

# AI Insights (optional - gracefully disabled if not set)
# Get key at: https://platform.openai.com/api-keys
OPENAI_API_KEY=YOUR_OPENAI_KEY_HERE
OPENAI_MODEL=gpt-4o
AI_ENABLED=true

# CORS & Security (optional)
ALLOWED_ORIGINS=http://localhost:8081,https://aidqa.vercel.app
RATE_LIMIT_PER_MINUTE=30
